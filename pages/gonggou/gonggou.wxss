.gonggou{
  width: 100%;
  height: auto;
}
.gg_box{
  width: 100%;
  height:auto;
  padding:0 10rpx;
  box-sizing:border-box;
}
.gg_left{
box-sizing: border-box;
  width: 30%;
  height:160rpx;
  float:left;
padding-top:20rpx;
  text-align: center;
}
.gg_right{
  width:70%;
  height:auto;
  float:right;
 
  
}
.gg_left image{
  width: 60%;
  height:82%;
  border-radius: 50%;
}
.gg_title{
  width: 100%;
  height:60rpx;
  line-height: 60rpx;
  font-size:0.8rem;
  color:#3283b0;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.information{
  width: 100%;
  height:120rpx;
  font-size:0.7rem;
  line-height:40rpx;
  overflow: hidden;
 
 
}
.detail_img{
  width: 100%;
  height:auto;
}
.detail_img .detail_img_box{
  width: 30%;
  height: 200rpx;
  display: inline-block;
}
.detail_img .detail_img_box image{
  width: 100%;
  height: 100%;
}
.bgc{
  background: #F2f2f2;
  width: 100%;
  height: 20rpx;
}
.publish{
  position: fixed; 
  top:30%;
  right:1rpx;
   z-index: 100; 
}