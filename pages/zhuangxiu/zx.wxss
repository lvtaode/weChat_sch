page{
  background: #F2f2f2;
}

.cs_list{
        box-sizing: border-box;
        width: 100%;
        height: auto;
        padding:10rpx;
    }
        .cs_list_box{
            box-sizing: border-box;
            width: 100%;
            height: 600rpx;
            border-radius: 10rpx 10rpx 0 0;
        }
        .cs_list_left{
            box-sizing: border-box;
            width: 46%;
            height: 100%;
          float:left;
            border-radius:  10rpx 10rpx 0 0;
            margin-left:20rpx;


    }
     .cs_list_left:nth-of-type(even){
        margin-left:10rpx;
    }
        .cs_left_top{
            box-sizing: border-box;
            width: 100%;
            height: auto;
            background-color: #fff;
            padding-bottom: 10rpx;
            border-radius:10rpx 10rpx  0 0;
        }
     .cs_left_top_text{
            box-sizing: border-box;
            width: 80%;
            height: 60rpx;
            background-color: #21B0FF;
            color:#fff;
            line-height: 60rpx;
            font-size:0.8rem;
            text-align: center;
            margin:0 auto;
            border-radius: 0 0 10rpx 10rpx;
        }
    .cs_list_right{
        box-sizing: border-box;
        width: 48%;
        height: 100%;
        float:right;
        border-radius:  10rpx 10rpx 0 0;

    }
        .cs_list_img{
            width: 100%;
            height: 240rpx;
        }
    .cs_list_img a{
        display: block;
        width: 100%;
        height: 100%;
    }
    .cs_list_img a img{
        width: 100%;
        height: 100%;
    }
        .cs_list_al{
            box-sizing: border-box;
            width: 100%;
            height: 60rpx;
            text-align: center;
            line-height: 60rpx;
            font-size:0.8rem;
            background: url("http://www.chinascw.net/public/images/cs_bj_2.png") no-repeat;
            background-size: 100% 100%;
        }
        .cs_list_al span{
            font-size:0.8rem ;
            color:#FF9228;
        }
        .cs_list-address{
            box-sizing: border-box;
            width: 100%;
            height: 160rpx;
            background: url("http://www.chinascw.net/public/images/cs_bj_1.png") no-repeat;
            background-size: 100% 100%;
            padding: 10rpx;
          
            line-height: 1.6;
        }
        .cs_list_address_box{
            box-sizing: border-box;
            width: 100%;
            overflow:hidden;
           text-overflow:ellipsis;
           white-space:nowrap;
           font-size:0.7rem;
        }
         .cs_list_address_box img{
             width: 24rem;
             margin-right: 6rpx;
        }
        .phone_personal{
  width: 100%;
  color:rgb(34, 154, 181);
  height:100rpx;
  line-height:100rpx;
  text-align: center;
 
}
.phone_one{
  display: flex; 
  position: relative;
  justify-content: space-between;
  background-color:#fff;
  width:100%;
  height:100rpx;
  margin:0 auto;
  border-radius: 10rpx;
  border-bottom:2rpx solid rgb(255, 255, 255);
  font-size:0.8rem;
}
.person_box{
  position: relative;
}
.phone_select{
  margin-top:0;
  z-index: 100;
  position: absolute; 
}
.select_one{
  text-align: center;
  background: #fff;
  width:676rpx;  
  line-height:100rpx;
  margin:0 5%;
  border-bottom:2rpx solid #ddd;
  font-size:0.8rem;
}
.personal_image{
  z-index: 100;
  position: absolute;
  right:2.5%;
  width: 34rpx;
  height: 20rpx;
  margin:40rpx 20rpx 40rpx 0;
  transition: All 0.4s ease; 
  -webkit-transition: All 0.4s ease;
}
.rotateRight{
  transform: rotate(180deg); 
}