/*新闻咨询  */
.news_information{
  width: 100%;
  height: 400rpx;
  box-sizing: border-box;
  padding:20rpx;
}
.news_left{
  box-sizing: border-box;
  width: 46%;
  height: 100%;
  float:left;
}
.news_right{
  box-sizing: border-box;
  width: 50%;
  height: 100%;
  float:right;
  margin-left:20rpx;
}
.swiper-container4{
  width: 100%;
  height: 100%;
 
}
.news_content{
  width: 100%;
  height: 50rpx;
  font-size: 0.8rem;
  line-height: 50rpx;
  background:#FFc540; 
  position: absolute;
  color:#fff;
  left:0;
  bottom:0;
  text-align: center;
}
.swiper_box4{
  height: 100%;
}
.news_list{
  width: 100%;
  height: 120rpx;
  border-bottom: 1px dashed #ddd;
}
.news_list text{
  display: block;
  padding:0 10rpx;
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.shop_information{
  box-sizing: border-box;
  width: 100%;
  height: auto;
  padding:0 60rpx;

}
.shop_information .shop_list{
  box-sizing: border-box;
  width: 50%;
  height: auto;
  display: inline-block;
  margin-bottom:20rpx;
}
.shop_information .shop_list image{
  height: 60rpx;
  width: 60rpx;
  border-radius: 50%;
  vertical-align: middle;
}
.shop_information .shop_list text{
  display: inline-block;
  width: 60%;
  font-size: 0.8rem;
  color:#777;
  overflow: hidden;
  text-overflow: ellipsis;
  vertical-align: middle;
  margin-left:10rpx;
}
.box{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.item{
