}
.service{
  position: fixed;
  z-index: 10000;
  width: 100rpx;
  height: 50px;
  padding: 0;
  right: 20rpx;
  top:160px;
}
.service image{
  padding: 0;
  display:block;
  margin: 0 auto;
}
.service button{
  padding: 0;
  width: 100rpx;
  height: 120rpx;
  font-size: 24rpx;
  border:none;
  color:red;
  background-color: transparent;
}
.search-view{
  z-index:500;
  width: 100%;
  height: 100rpx;
  display: flex;
  padding:20rpx;
  box-sizing: border-box;
}
.search_left{
width:20%;
height:50rpx;
line-height: 35rpx;
font-size:25rpx;
color:#666;
margin-right:20rpx;
background:#CECECE;
text-align:center;
margin: auto 10rpx;
margin-left: -10rpx;
border-radius:20rpx;
}
.search_right{
  width: 250rpx;
  font-size:25rpx;
}
.search_box{
  width:600rpx;
   height: 60rpx;
   box-sizing: border-box;
   margin: auto 30rpx; 
  border:1px solid #000;
  border-radius: 30rpx;
  position: relative;
}
.search{
  width: 300rpx;
 height: 60rpx;
  margin: 0 auto;
  line-height: 60rpx;
  font-size: 26rpx;
  color:#000;
}
.search-img{
  position: absolute;
  left: 10rpx;
  top: 16rpx;
}
.sousuo{
  display: block;
  background-color: #3283B0;
  border-radius: 0 30px 30px 0;
  color:#fff;
  height: 100%;
  line-height: 55rpx;
  font-size: 26rpx;
  width:70rpx;
  text-align: center;
  border-left:1px solid #000; 
  position: absolute;
  right:-1px; 
  top:0;
}
.search_left navigator{
  display: block;
  box-sizing: border-box;
  width: 100%;
  height: 100%;
 border-radius: 10rpx;
 padding:10rpx;
 overflow:hidden; 
  text-overflow:ellipsis;
  white-space:nowrap;  

}

.coupons{
  width: 100%;
  height: 200rpx;
}
.coupons-scroll{
  white-space:nowrap;
  width: 100%;
  height: 100%;
  
}
.cou-box{
  width: 160rpx;
  height: 100%;
  border: 1px solid red;
 display: inline-block;
}
