.gonggou{
  width: 100%;
  height: auto;

}
.gg_box{
  width: 100%;
  height:auto;
}
.gg_left{
  box-sizing: border-box;
  width: 30%;
  height:160rpx;
  float:left;
padding-top:40rpx;
  text-align: center;
}
.gg_right{
  width:70%;
  height:auto;
  float:right;
 
  
}

 .gg_left image{
  width: 60%;
  height:82%;
  
}

.gg_title{
  width: 100%;
  height:60rpx;
  line-height: 60rpx;
  font-size:0.8rem;
  color:#3283b0;
}
.information{
  width: 100%;
  height:120rpx;
  font-size:0.7rem;
  line-height:40rpx;
  overflow: hidden;
}
.detail_img{
  box-sizing: border-box;
  width: 100%;
  height:auto;
  padding:70rpx;
}
.detail_img .detail_img_box{
  width: 30%;
  height: 200rpx;
  display: inline-block;
  margin-left:20rpx;

}
.detail_img .detail_img_box image{
  width: 100%;
  height: 100%;
}
.bgc{
  background: #F2f2f2;
  width: 100%;
  height: 10rpx;
}
.contact,.tell{
  width: 100%;
  height:40rpx;
  line-height: 40rpx;
  font-size: 0.7rem;
}
.gg_detail{
  width: 100%;
  height:auto;
}
.gg_detail_title{
  width: 100%;
  height: auto;
  line-height: 56rpx;
  font-size:0.9rem;
  text-align: center;
  margin-top:20rpx;
  box-sizing:border-box;
padding:0 20rpx;

}
.dd_detail_time{
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  font-size:0.65rem;
  text-align: center;
  color:#ddd;
  margin:16rpx 0;
}
.dd-detail_information{
 box-sizing:border-box;
padding:0 20rpx;
width:100%;
height:auto;
font-size:0.8rem;
line-height:56rpx;
color:#777;

}