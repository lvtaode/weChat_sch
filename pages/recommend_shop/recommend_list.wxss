/*列表页  */
.recommend_list{
  width: 100%;
  height: 180rpx;
  box-sizing: border-box;
  padding:20rpx;
  border-bottom:1px solid #ddd;
  border-top:1px solid #ddd;
}
.recommend_img{
  width: 40%;
  height: 100%;
  
  display: inline-block;
  box-sizing: border-box;
}
.recommend_img image{
  width: 86%;
  height: 100%;
}
.recommend_content{
  width: 58%;
  height: 100%;
  
  float:right;
 margin-left:10rpx;
  box-sizing: border-box;
}
.recommend_title{
  width: 100%;
  font-size: 0.8rem;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.recommend_adr{
  width: 100%;
  box-sizing: border-box;
  font-size:0.72rem;
  height: 40rpx;
  line-height: 40rpx;
  color:#666;
}
.recommend_left{
  width: 60%;
  height: 100%;
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.recommend_right{
  width: 40%;
  height: 100%;
  display: inline-block;
}
.recommend_right .tjqy{
background: #85CEFB;
  color:#fff;
  width: 100rpx;
  height: 40rpx;
  text-align: center;
  font-size:0.6rem;
  line-height: 40rpx;
  float:right;
  border-radius: 10rpx;
}
.recommend_intro{
  width:100%;
height:60rpx;
font-size:0.6rem;
color:#777;
overflow:hidden;
line-height:30rpx;
padding:5rpx;

}
.bgc{
  background: #F2f2f2;
  width: 100%;
  height: 20rpx;
}
.phone_personal{
  width: 100%;
  color:rgb(34, 154, 181);
  height:100rpx;
  line-height:100rpx;
  text-align: center;
}
.phone_one{
  display: flex; 
  position: relative;
  justify-content: space-between;
  background-color:#fff;
  width:50%;
  height:100rpx;
  border-radius: 10rpx;
  border-bottom:2rpx solid rgb(255, 255, 255);
  font-size:0.8rem;
  float:left;
}
.person_box{
  position: relative;
}
.phone_select{
  width: 100%;
  margin-top:57px;
  z-index: 100;
  position: absolute; 
}
.select_one{
  text-align: center;
  background: #fff;
  /* width:676px;   */
  line-height:100rpx;
  border-bottom:2rpx solid #ddd;
  font-size:0.8rem;
}
.personal_image{
  z-index: 100;
  position: absolute;
  right:2.5%;
  width: 34rpx;
  height: 20rpx;
  margin:40rpx 20rpx 40rpx 0;
  transition: All 0.4s ease; 
  -webkit-transition: All 0.4s ease;
}
.rotateRight{
  transform: rotate(180deg); 
}